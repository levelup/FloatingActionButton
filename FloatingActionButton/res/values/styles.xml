<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Dimensions according to the Material guidelines http://www.google.com/design/spec/patterns/promoted-actions.html#promoted-actions-floating-action-button  -->
    <style name="FABLarge">
        <item name="android:layout_width">@dimen/fab_default_size_large</item>
        <item name="android:layout_height">@dimen/fab_default_size_large</item>
        <item name="android:layout_marginBottom">@dimen/fab_default_margin_large</item>
        <item name="android:layout_marginRight">@dimen/fab_default_margin_large</item>
        <item name="fab_elevation">@dimen/fab_default_elevation</item>
        <item name="fab_elevationPressed">@dimen/fab_default_elevationPressed</item>
        <item name="fab_duration">@integer/fab_default_duration</item>
    </style>

    <!-- Dimensions according to the Material guidelines http://www.google.com/design/spec/patterns/promoted-actions.html#promoted-actions-floating-action-button  -->
    <style name="FABMedium">
        <item name="android:layout_width">@dimen/fab_default_size_medium</item>
        <item name="android:layout_height">@dimen/fab_default_size_medium</item>
        <item name="android:layout_marginBottom">@dimen/fab_default_margin_medium</item>
        <item name="android:layout_marginRight">@dimen/fab_default_margin_medium</item>
        <item name="fab_elevation">@dimen/fab_default_elevation</item>
        <item name="fab_elevationPressed">@dimen/fab_default_elevationPressed</item>
        <item name="fab_duration">@integer/fab_default_duration</item>
    </style>
</resources>